%link{:crossorigin => "anonymous", :href => "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css", :integrity => "sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7", :rel => "stylesheet"}/

.container
  .jumbotron

    %h2 Edit Athlete Info
    = form_for @athlete do |f|
      = f.label :name
      = f.text_field :name
      = f.label :height
      = f.text_field :height
      = f.label :weight
      = f.text_field :weight
      = f.label :age
      = f.text_field :age
      = f.label :location
      = f.text_field :location
      = f.label :image_path
      = f.text_field :image_path
      %button.btn.btn-primary.active{:type => "submit"}= f.submit "Edit Athlete Info"
